{
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --color --progress",
    "gen": "ts-node --project ./tsconfig.json ./scripts/gen.ts",
    "build": "shx rm -rf dist && cross-env NODE_ENV=production webpack --color --progress && shx cp README.md dist && cd dist && bestzip ../PluginZip.zip ./*",
    "build-watch": "nodemon --watch 'src/**/* public/**/*' --exec \"npm run build\" -e '*'",
    "start": "http-server --cors -p 7000 dist -c-1"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "dependencies": {
    "@remnote/plugin-sdk": "0.0.28",
    "dayjs": "^1.10.8",
    "mathjs": "11.2.1",
    "mustache": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remeda": "0.0.32"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "@types/mathjs": "^9.4.2",
    "@types/mustache": "^4.1.2",
    "babel-loader": "^8.2.3",
    "clsx": "^1.1.1",
    "css-loader": "^6.7.1",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/node": "^18.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^2.19.0",
    "html-webpack-plugin": "^5.5.0",
    "mathjs": "11.2.1",
    "postcss": "^8.4.14",
    "shx": "^0.3.4",
    "bestzip": "^2.2.1",
    "react-refresh": "^0.14.0",
    "serve": "^14.0.1",
    "tailwindcss": "^3.1.5",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "ts-node": "^10.9.1"
  }
}
